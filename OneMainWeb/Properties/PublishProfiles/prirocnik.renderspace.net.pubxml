            <Source Path="$(IntermediateOutputPath)AutoScripts\MsSqlConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=kths5c9mo5.database.windows.net;User ID=zrsz;Password=Uhx9aDwCn5mVJb6Z!;Initial Catalog=zrsz;Pooling=true;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
